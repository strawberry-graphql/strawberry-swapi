generator db {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

datasource db {
  provider = "sqlite"
  url      = "file:db.sqlite3"
}

model Film {
  id            Int      @id
  title         String
  episode_id    Int
  opening_crawl String
  director      String
  producers     String
  release_date  DateTime
  created       DateTime
  edited        DateTime
}

model Person {
  id         Int      @id
  name       String
  height     Int?
  mass       Float?
  eye_color  String
  birth_year String
  gender     String
  hair_color String
  skin_color String
  created    DateTime
  edited     DateTime
}
