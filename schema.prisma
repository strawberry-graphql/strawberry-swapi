generator db {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

datasource db {
  provider = "sqlite"
  url      = "file:db.sqlite3"
}

model Film {
  id            Int      @id
  title         String
  episode_id    Int
  opening_crawl String
  director      String
  producers     String
  release_date  DateTime
}
